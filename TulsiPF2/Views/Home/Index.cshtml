@{
    ViewBag.Title = "Home Page";

    
}

<div class="glyphicon-adjust">
    <h2>Tulsi Phuloria Foundation</h2>
</div>

<div class="row">
    <div class="col-md-6">

        <div id="mycarousel" class="carousel slide" data-ride="carousel" style="margin-top:auto">
            <ol class="carousel-indicators">
                <li data-target="#mycarousel" data-slide-to="0" class="active"></li>
                <li data-target="#mycarousel" data-slide-to="1"> </li>
                <li data-target="#mycarousel" data-slide-to="2"> </li>
            </ol>

            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="~/ImageLogo/TulsiF.jpg" style="width:650px; height:400px" />
                </div>
               
                <div class="item">
                    <img src="~/ImageLogo/TulsiF2.jpg" style="width:650px; height:400px" />
                </div>

                <div class="item">
                    <img src="~/ImageLogo/TulsiF3.jpg" style="width:650px; height:400px" />
                </div>
            </div>

            <a class="left carousel-control" href="#mycarousel" role="button" data-slide="prev">
                <span class="grahicon grahicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>

            <a class="right carousel-control" href="#mycarousel" role="button" data-slide="next">
                <span class="grahicon grahicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>

            @*<a href="@Url.Action("LogOut", "User")">Logout </a>*@

        </div>
    </div>
</div>
